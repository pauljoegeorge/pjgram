<div class="container">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="row">
      <form class="col s12">
        <div class="row">
          <div class="col s12 m4 l5"></div>
          <div class="col s12 m4 l2"><h2>Login</h2></div>
          <div class="col s12 m4 l5"></div>
        </div>
        <div class="row">
          <div class="col s12 m4 l3"><p></p></div>
          <div class="input-field col s12 m4 l6">
            <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
            <label for="email">Email</label>
          </div>
          <div class="col s12 m4 l3"><p></p></div>
        </div>
        <div class="row">
          <div class="col s12 m4 l3"><p></p></div>
          <div class="input-field col s12 m4 l6">
            <%= f.password_field :password, autocomplete: "current-password" %>
            <label for="password">Password</label>
          </div>
          <div class="col s12 m4 l3"><p></p></div>
        </div>
        <% if devise_mapping.rememberable? %>
          <div class="row">
            <div class="col s12 m4 l3"><p></p></div>
            <div class="input-field col s12 m4 l6">
              <label>
                <%= f.check_box :remember_me,  class: "filled-in" %>
                <span>Remember Me</span>
              </label>
            </div>
            <div class="col s12 m4 l3"><p></p></div>
          </div>
        <% end %>
        <div class="row">
          <div class="col s12 m4 l3"><p></p></div>
          <div class="input-field col s12 m4 l6">
            <%= f.submit "Log in",  class: "waves-effect waves-light btn-large #3f51b5 indigo" do %>
              <i class="material-icons left">exit_to_app</i>
            <% end %>
          </div>
          <div class="col s12 m4 l3"><p></p></div>
        </div>
      </form>
    </div>
  <%end %>
  <%= render "devise/shared/links" %>
</div>
